class Demo {
	
	 public void animal()
	{
		System.out.println("Barks....");
	}
class Dog extends Demo{
	
	public void color()
	{
		System.out.println("brown");
	}

class Inheritance{	

	public static void main(String args[]) 
	{
		// TODO Auto-generated method stub
		Dog d = new Dog();
		d.animal();
		
	}

}
}
}